<section>
  <link rel="stylesheet" href="/public/css/homepage.css">
  <link rel="stylesheet" href="/public/css/newcar.css">
  <div class="bigtitle">
      <h1 style="text-align:center">USED CARS</h1>
      <div class="newcar-container">
      {{#each cars}}  
        {{#if Usedcar}}
          <p> <option value="{{_id}}">VIN:{{VIN}} || Brand:{{Brand}} || Model:{{Model}} || VehicleType:{{VehicleType}} || Timetomarket:{{Timetomarket}} || Color:{{Color}}</option></p> 
          <img src="/public/images/{{img}}">
        {{/if}}
      {{/each}}
		</div>
  </div>
</section>